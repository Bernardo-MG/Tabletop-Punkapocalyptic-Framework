<?xml version="1.0" encoding="UTF-8"?>
<jasperReport name="FirstJasper">
	<field name="unitName" class="java.lang.String"/>
	<field name="actions" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="agility" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="combat" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="precision" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="strength" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="tech" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<field name="toughness" class="com.wandrell.tabletop.business.model.valuehandler.ValueHandler"/>
	<group name="unit_data">
		<groupExpression><![CDATA[$F{unitName}]]></groupExpression>
		<groupHeader>
			<band height="23">
				<staticText>
					<reportElement mode="Opaque" x="0" y="0" width="535" height="15" backcolor="#70A9A9"/>
					<box>
						<bottomPen lineWidth="1.0" lineColor="#CCCCCC"/>
					</box>
					<text><![CDATA[]]></text>
				</staticText>
				<textField>
					<reportElement x="0" y="0" width="136" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle"/>
					<textFieldExpression><![CDATA[$F{unitName}]]></textFieldExpression>
				</textField>
			</band>
		</groupHeader>
	</group>
	<group name="unit_attributes">
		<groupHeader>
			<band height="23">
				<staticText>
					<reportElement mode="Opaque" x="0" y="0" width="535" height="15" backcolor="#70A9A9"/>
					<box>
						<bottomPen lineWidth="1.0" lineColor="#CCCCCC"/>
					</box>
					<text><![CDATA[]]></text>
				</staticText>
				<staticText>
					<reportElement x="0" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Actions]]></text>
				</staticText>
				<staticText>
					<reportElement x="60" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Combat]]></text>
				</staticText>
				<staticText>
					<reportElement x="120" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Precision]]></text>
				</staticText>
				<staticText>
					<reportElement x="180" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Agility]]></text>
				</staticText>
				<staticText>
					<reportElement x="240" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Strength]]></text>
				</staticText>
				<staticText>
					<reportElement x="300" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Tough.]]></text>
				</staticText>
				<staticText>
					<reportElement x="360" y="0" width="60" height="15"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font isBold="true"/>
					</textElement>
					<text><![CDATA[Tech]]></text>
				</staticText>
			</band>
		</groupHeader>
		<groupFooter>
			<band height="23"/>
		</groupFooter>
	</group>
	<detail>
		<band height="16">
			<staticText>
				<reportElement mode="Opaque" x="0" y="0" width="535" height="14" backcolor="#E5ECF9"/>
				<box>
					<bottomPen lineWidth="0.25" lineColor="#CCCCCC"/>
				</box>
				<text><![CDATA[]]></text>
			</staticText>
			<textField>
				<reportElement x="0" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{actions}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="60" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{combat}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="120" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{precision}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="180" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{agility}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="240" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{strength}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="300" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{toughness}.getStoredValue()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="360" y="0" width="60" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle"/>
				<textFieldExpression><![CDATA[$F{tech}.getStoredValue()]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
